    <div class="col-sm-3  container">

      <form class="form-signin" action="/mobile/input" method="post" role="form">
        <h5 class="form-signin-heading">データ送信</h5>
        <input type="text" name="userid" class="form-control" placeholder="ユーザーID" required>
        <input type="password" name="password" class="form-control" placeholder="Password" required>
        <input type="text" name="latitude" class="form-control" placeholder="緯度" required>
        <input type="text" name="longitude" class="form-control" placeholder="経度" required>
        <input type="text" name="title" class="form-control" placeholder="場所名" required>
        <input type="text" name="status" class="form-control" placeholder="Good or Bad ?" required>
        <input type="text" name="description" class="form-control" placeholder="何があった?" required>
        <br>
